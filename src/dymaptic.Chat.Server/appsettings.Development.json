{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Extensions.Hosting": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "ExcludedUrlStrings": [
      "/images",
      "/css",
      ".css",
      "_framework",
      "_blazor",
      "/scripts",
      ".js",
      "/favicon.ico"
    ],
    "LoggingFolder": "Logs"
  }
}
